<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="JAZZZ3b_PU" transaction-type="JTA">
    <jta-data-source>java:app/jdbc/winkelversie3</jta-data-source>
    <class>entities.Account</class>
    <class>entities.Adres</class>
    <class>entities.Artikel</class>
    <class>entities.Bestelling</class>
    <class>entities.Bestellingartikel</class>
    <class>entities.Betaling</class>
    <class>entities.Factuur</class>
    <class>entities.Klant</class>
    <class>entities.Klantadres</class>
    <class>entities.Medewerker</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
      <property name="eclipselink.logging.level" value="FINEST"/>
    </properties>
  </persistence-unit>
</persistence>
